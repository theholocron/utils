  },
  "dependencies": {
    "change-case": "^5.4.4",
    "title-case": "^4.3.1"
  },
  "devDependencies": {
    "@theholocron/eslint-config": "^3.5.1",
    "@theholocron/jest-config": "^3.5.1",
    "@theholocron/prettier-config": "^3.5.1",
    "@theholocron/tsconfig": "^3.5.1",
    "@tsconfig/node-lts": "^20.1.3",
    "vite": "^5.4.9"
  },
  "packageManager": "npm@9.5.0",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
